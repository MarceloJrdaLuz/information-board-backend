{
  "version": 2,
  "builds": [
    {
      "src": "dist/index.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "dist/index.js"
    }
  ],
  "crons": [
    {
      "path": "/deleteExpiredNotices",
      "schedule": "0 0 * * 1"
    }, 
    {
      "path": "/reportsCleanUp", 
      "schedule": "0 0 1 1 *"
    },
    {
      "path": "/backup", 
      "schedule": "0 0 * * 2"
    }
  ]
}