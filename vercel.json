{
  "version": 2,
  "builds": [
    {
      "src": "./dist/api/index.js",
      "use": "@now/node", 
      "config": { "includeFiles": ["dist/**"] }
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "./dist/api/index.js"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ]
}