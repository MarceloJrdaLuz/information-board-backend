{
  "version": 2,
  "builds": [
    {
      "src": "api",
      "use": "@now/node"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "./api"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ]
}